{
  "manifest_version": 3,
  "name": "Time2Read Estimator",
  "version": "0.0.1",
  "description": "Time2Read Estimator is an extension that estimates the time to read articles/ documentations on websites",
  "icons": {
    "16": "images/T2R1.png",
    "32": "images/T2R1.png",
    "48": "images/T2R1.png",
    "128": "images/T2R1.png"
  },
  "action": {
    "default_icon": {
      "16": "images/T2R1.png",
      "32": "images/T2R1.png",
      "48": "images/T2R1.png",
      "128": "images/T2R1.png"
    }
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "<all_urls>"
      ],
      "type": "module"
    }
  ]
}
